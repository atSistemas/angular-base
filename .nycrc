{
    "include": [
    "src/**/*.ts"
  ],
  "exclude": [
    "node_modules/",
    "dist",
    "webpack",
    "**/**.spec.ts",
    "src/base/conf",
    "src/base/effects/index.ts",
       "src/base/providers/index.ts",
      "src/base/services/index.ts",
    "src/base/wp-plugins",
    "src/app/**/*.ts"
  ],
  "extension": [
    ".ts"
  ],
  "require": [
    "ts-node/register"
  ],
  "reporter": [
    "text",
    "text-summary",
    "html"
  ],
  "all": true,
  "sourceMap": true,
  "instrument": true
}