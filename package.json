  },
  "nyc": {
   "exclude": [
     "node_modules/",
     "dist/"
   ],
   "extension": [
     ".ts",
     ".tsx"
   ],
   "require": [
     "ts-node/register"
   ],
   "reporter": [
     "text-summary",
     "html"
   ],
   "sourceMap": true,
   "instrument": true
 }
}
