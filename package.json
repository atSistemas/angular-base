    "@types/express": "^4.0.34",
    "@types/mocha": "^2.2.38",
    "@types/node": "^6.0.50",
    "@types/redux-logger": "^2.6.32",
    "@types/sinon": "^1.16.34",
    "@types/systemjs": "^0.19.31",
    "@types/webpack": "^2.0.0",
