{
  "name": "Angular2Base",
  "version": "0.9.6",
  "description": "Angular2 Universal starterkit",
  "author": "Pablo Magaz",
  "contributors": [
    {
      "name": "Santiago Fernandez blanco",
      "email": "sfernandez.blanco@atsistemas.com"
    },
    {
      "name": "Santiago Fernandez blanco",
      "email": "sfernandez.blanco@atsistemas.com"
    }
  ],
  "scripts": {
    "logo": "node ./src/base/conf/.angular2-base/logo.ts",
    "webpack": "node_modules/webpack/bin/webpack.js",
    "typings": "node_modules/typings/dist/bin.js",
    "ngc": "./node_modules/.bin/ngc -p tsconfig.aot.json",
    "generate": "npm run logo && yo react-base",
    "generate:component": "npm run logo && yo react-base --option=2",
    "generate:container": "npm run logo && yo react-base --option=1",
    "regenerate:modelIndex": "node ./src/base/shared/Regenerate.js --model",
    "regenerate:routes": "node ./src/base/shared/Regenerate.js --routes",
    "regenerate:reducerIndex": "node ./src/base/shared/Regenerate.js --reducer",
    "regenerate": "npm run logo && node ./src/base/shared/Regenerate.js --all",
    "start": "npm run logo && cross-env NODE_ENV=development node server/",
    "start:prod": "npm run build:prod && cross-env NODE_ENV=production node server/",
    "build": "cross-env NODE_ENV=development webpack --config webpack/index.ts",
    "build:prod": "npm run ngc && cross-env NODE_ENV=production webpack --config webpack/index.ts",
    "generate:dll": "npm run logo && cross-env NODE_ENV=production webpack --config webpack/webpack.dll.config.ts",
    "clean": "rimraf dist/ && rimraf compiled",
    "tslint": "tslint --exclude=src/**/*.spec.ts",
    "lint": "npm run tslint \"src/**/*.ts\"",
    "lint:css": "npm run logo && stylelint \"src/**/*.css\" --config .stylelintrc",
    "lint:all": "npm run lint && npm run lint:css",
    "test": "npm run logo && rimraf .tmp && mocha-webpack --webpack-config webpack/webpack.test.config.ts --require ./src/base/conf/jsDomSetup.js src/**/*.spec.ts",
    "test:watch": "rimraf .tmp && mocha-webpack --watch",
    "test:coverage": "nyc npm test && cross-env NODE_ENV=development",
    "postYeomanGenerator": "npm run regenerate && npm run start",
    "yo": "npm link src/conf/.react-base/templates/generator-react-base",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/.bin/coveralls",
    "postinstall1": "npm run clean && npm run generate:dll"
  },
  "dependencies": {
    "@angular/animations": "5.0.3",
    "@angular/common": "5.0.3",
    "@angular/compiler": "5.0.3",
    "@angular/compiler-cli": "5.0.3",
    "@angular/core": "5.0.3",
    "@angular/http": "5.0.3",
    "@angular/platform-browser": "5.0.3",
    "@angular/platform-browser-dynamic": "5.0.3",
    "@angular/platform-server": "5.0.3",
    "@angular/router": "5.0.3",
    "@angular/tsc-wrapped": "4.4.6",
    "@angularclass/hmr": "2.1.3",
    "@angularclass/hmr-loader": "3.0.4",
    "@angularclass/idle-preload": "2.0.1",
    "@ngrx/core": "1.2.0",
    "@ngrx/effects": "4.1.1",
    "@ngrx/router-store": "4.1.1",
    "@ngrx/store": "4.1.1",
    "@ngrx/store-devtools": "4.1.1",
    "angular-router-loader": "0.8.0",
    "angular2-template-loader": "0.6.2",
    "body-parser": "1.18.2",
    "compression": "1.7.1",
    "core-js": "2.5.1",
    "express": "4.16.2",
    "ngrx-store-freeze": "0.2.0",
    "ngrx-store-logger": "0.2.0",
    "zone.js": "0.8.18"
  },
  "devDependencies": {
    "@types/chai": "4.0.5",
    "@types/core-js": "0.9.43",
    "@types/express": "4.0.39",
    "@types/mocha": "2.2.44",
    "@types/node": "8.0.53",
    "@types/redux-logger": "3.0.5",
    "@types/sinon": "4.0.0",
    "@types/systemjs": "0.20.6",
    "@types/webpack": "3.8.1",
    "add-asset-html-webpack-plugin": "2.1.2",
    "assets-webpack-plugin": "3.5.1",
    "awesome-typescript-loader": "3.4.0",
    "chai": "4.1.2",
    "chalk": "2.3.0",
    "clean-css": "4.1.9",
    "compression-webpack-plugin": "1.0.1",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "5.1.1",
    "css-loader": "0.28.7",
    "custom-tslint-formatters": "2.1.1",
    "es6-shim": "^0.35.3",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.5",
    "helpers": "0.0.6",
    "html-loader": "^0.5.1",
    "html-webpack-externals-plugin": "3.6.0",
    "html-webpack-plugin": "^2.28.0",
