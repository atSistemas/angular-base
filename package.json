    "run-sequence": "2.2.0",
    "rxjs": "5.5.2",
    "sinon": "4.1.2",
    "string-replace-loader": "1.3.0",
    "style-loader": "0.19.0",
    "to-string-loader": "1.1.5",
    "ts-helpers": "github:gdi2290/ts-helpers",
    "ts-node": "3.3.0",
    "tslint": "5.8.0",
    "tslint-eslint-rules": "4.1.1",
    "tslint-immutable": "^4.4.0",
    "typescript": "2.4.2",
    "url-loader": "0.6.2",
    "webpack": "3.8.1",
    "webpack-dev-middleware": "1.12.1",
    "webpack-dev-server": "2.9.4",
    "webpack-dll-bundles-plugin": "1.0.0-beta.5",
    "webpack-hot-middleware": "2.21.0",
    "webpack-node-externals": "1.6.0"
  },
  "nyc": {
    "include": [
      "src/**/*.ts",
      "src/**/*.tsx"
    ],
    "exclude": [
      "node_modules/",
      "compiled",
      "dist/",
      "webpack/**",
      "**/*.spec.js",
      "src/base/wp-plugins/**"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text",
      "text-summary",
      "html"
    ],
    "all": true,
    "sourceMap": true,
    "instrument": true
  }
}
