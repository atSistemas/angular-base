{
  "name": "Angular2Base",
  "version": "0.9.5",
  "description": "Angular2 Universal starterkit",
  "author": "Pablo Magaz",
  "contributors": [
    {
      "name": "Rafael Bernad",
      "email": "rbernad@atsistemas.com"
    },
    {
      "name": "David del Prado Martinez",
      "email": "ddelprado@atsistemas.com"
    },
    {
      "name": "Jose Carlos Lama",
      "email": "jclama@atsistemas.com"
    }
  ],
  "scripts": {
    "logo": "node ./src/base/conf/.angular2-base/logo.ts",
    "webpack": "node_modules/webpack/bin/webpack.js",
    "typings": "node_modules/typings/dist/bin.js",
    "ngc": "./node_modules/.bin/ngc -p tsconfig.aot.json",
    "generate": "npm run logo && yo react-base",
    "generate:component": "npm run logo && yo react-base --option=2",
    "generate:container": "npm run logo && yo react-base --option=1",
    "regenerate:modelIndex": "node ./src/base/shared/Regenerate.js --model",
    "regenerate:routes": "node ./src/base/shared/Regenerate.js --routes",
    "regenerate:reducerIndex": "node ./src/base/shared/Regenerate.js --reducer",
    "regenerate": "npm run logo && node ./src/base/shared/Regenerate.js --all",
    "start": "npm run logo && cross-env NODE_ENV=development node server/",
    "start:prod": "npm run build:prod && cross-env NODE_ENV=production node server/",
    "build": "cross-env NODE_ENV=development webpack --config webpack/index.ts",
    "build:prod": "npm run ngc && cross-env NODE_ENV=production webpack --config webpack/index.ts",
    "generate:dll": "npm run logo && cross-env NODE_ENV=production webpack --config webpack/webpack.dll.config.ts",
    "clean": "rimraf dist/ && npm run generate:dll",
    "tslint": "tslint --exclude=src/**/*.spec.ts",
    "lint": "npm run tslint \"src/**/*.ts\"",
    "lint:css": "npm run logo && stylelint \"src/**/*.css\" --config .stylelintrc",
    "lint:all": "npm run lint && npm run lint:css",
    "test": "npm run logo && rimraf .tmp && mocha-webpack --webpack-config webpack/webpack.test.config.ts --require ./src/base/conf/jsDomSetup.js src/**/*.spec.ts",
    "test:watch": "rimraf .tmp && mocha-webpack --watch",
    "test:coverage": "nyc npm test && nyc report --reporter=lcov",
    "postYeomanGenerator": "npm run regenerate && npm run start",
    "yo": "npm link src/conf/.react-base/templates/generator-react-base",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/.bin/coveralls",
    "postinstall": "npm run generate:dll"
  },
  "dependencies": {
    "@angular/common": "4.0.0",
    "@angular/compiler": "4.0.0",
    "@angular/compiler-cli": "4.0.0",
    "@angular/core": "4.0.0",
    "@angular/http": "4.0.0",
    "@angular/platform-browser": "4.0.0",
    "@angular/platform-browser-dynamic": "4.0.0",
    "@angular/platform-server": "4.0.0",
    "@angular/router": "4.0.0",
    "@angular/tsc-wrapped": "4.0.0",
    "@angularclass/hmr": "1.2.2",
    "@angularclass/hmr-loader": "3.0.2",
    "@angularclass/idle-preload": "^1.0.4",
    "@angularclass/resolve-angular-routes": "^1.0.9",
    "@ngrx/core": "^1.2.0",
    "@ngrx/effects": "^2.0.0",
    "@ngrx/router-store": "^1.2.5",
    "@ngrx/store": "^2.2.1",
    "@ngrx/store-devtools": "^3.2.3",
    "@ngrx/store-log-monitor": "^3.0.2",
    "angular-router-loader": "0.5.0",
    "angular2-express-engine": "^2.1.0-rc.1",
    "angular2-template-loader": "^0.6.0",
    "angular2-universal": "^2.1.0-rc.1",
    "angular2-universal-polyfills": "^2.1.0-rc.1",
    "body-parser": "^1.15.2",
    "compression": "^1.6.2",
    "core-js": "2.4.1",
    "express": "^4.14.0",
    "zone.js": "0.8.5"
  },
  "devDependencies": {
    "@types/chai": "^3.4.34",
    "@types/core-js": "^0.9.38",
    "@types/express": "^4.0.34",
    "@types/mocha": "^2.2.38",
    "@types/node": "6.0.50",
    "@types/redux-logger": "^2.6.32",
    "@types/sinon": "^1.16.34",
    "@types/systemjs": "^0.19.31",
    "@types/webpack": "^2.2.4",
    "add-asset-html-webpack-plugin": "^1.0.2",
    "assets-webpack-plugin": "^3.5.0",
    "awesome-typescript-loader": "2.2.4",
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.1",
    "babel-preset-es2015": "^6.18.0",
    "babili-webpack-plugin": "0.0.11",
    "chai": "^3.5.0",
    "chalk": "^1.1.3",
    "clean-css": "^3.4.21",
    "compression-webpack-plugin": "^0.3.2",
    "cross-env": "^3.1.3",
    "custom-tslint-formatters": "^1.1.1",
    "helpers": "0.0.6",
    "html-webpack-plugin": "^2.28.0",
    "ignore-loader": "^0.1.2",
    "immutable": "^3.8.1",
    "istanbul-instrumenter-loader": "^1.0.0",
    "jsdom": "^9.9.1",
    "json-loader": "^0.5.4",
    "mocha": "^3.2.0",
    "mocha-webpack": "^0.7.0",
    "ngrx-store-freeze": "^0.1.6",
    "ngrx-store-logger": "^0.1.7",
    "nyc": "^10.1.2",
    "path-to-regexp": "^1.7.0",
    "perfy": "^1.1.2",
    "progress-bar-webpack-plugin": "^1.9.0",
    "raw-loader": "^0.5.1",
    "reflect-metadata": "0.1.10",
    "reselect": "^2.5.4",
    "rimraf": "^2.5.4",
    "run-sequence": "^1.2.2",
    "rxjs": "5.2.0",
    "sinon": "^1.17.7",
    "string-replace-loader": "^1.0.5",
    "to-string-loader": "^1.1.5",
    "ts-helpers": "github:gdi2290/ts-helpers",
    "ts-node": "^1.7.0",
    "tslint": "4.5.1",
    "typed-immutable-record": "^0.0.6",
    "typescript": "2.1.1",
    "typings": "^2.0.0",
    "webpack": "2.3.1",
    "webpack-dashboard": "0.3.0",
    "webpack-dev-middleware": "^1.9.0",
    "webpack-dev-server": "^2.2.0",
    "webpack-dll-bundles-plugin": "^1.0.0-beta.5",
    "webpack-hot-middleware": "^2.16.0",
    "webpack-node-externals": "^1.5.4"
  },
  "nyc": {
    "include": [
      "src/**/*.ts",
      "src/**/*.tsx"
    ],
    "exclude": [
      "node_modules/",
      "src/compiled",
      "dist/",
      "webpack/**",
      "**/*.spec.js",
      "src/base/wp-plugins/**"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "all": true,
    "sourceMap": true,
    "instrument": true
  }
}
