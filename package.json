    "@types/core-js": "0.9.41",
    "@types/express": "4.0.35",
    "@types/mocha": "2.2.40",
    "@types/node": "7.0.12",
    "@types/redux-logger": "3.0.0",
    "@types/sinon": "2.1.1",
    "@types/systemjs": "0.20.2",
    "@types/webpack": "2.2.14",
    "add-asset-html-webpack-plugin": "1.0.2",
