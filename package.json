{
  "name": "Angular2Base",
  "version": "0.9.6",
  "description": "Angular2 Universal starterkit",
  "author": "Pablo Magaz",
  "contributors": [
    {
      "name": "Rafael Bernad",
      "email": "rbernad@atsistemas.com"
    },
    {
      "name": "Gaston Messeri",
      "email": "gemesseri@atsistemas.com"
    },
    {
      "name": "Jose Carlos Lama",
      "email": "jclama@atsistemas.com"
    },
    {
      "name": "David del Prado Martinez",
      "email": "ddelprado@atsistemas.com"
    }
  ],
  "scripts": {
    "logo": "node ./src/base/conf/.angular2-base/logo.ts",
    "webpack": "node_modules/webpack/bin/webpack.js",
    "typings": "node_modules/typings/dist/bin.js",
    "ngc": "./node_modules/.bin/ngc -p tsconfig.aot.json",
    "generate": "npm run logo && yo react-base",
    "generate:component": "npm run logo && yo react-base --option=2",
    "generate:container": "npm run logo && yo react-base --option=1",
    "regenerate:modelIndex": "node ./src/base/shared/Regenerate.js --model",
    "regenerate:routes": "node ./src/base/shared/Regenerate.js --routes",
    "regenerate:reducerIndex": "node ./src/base/shared/Regenerate.js --reducer",
    "regenerate": "npm run logo && node ./src/base/shared/Regenerate.js --all",
    "start": "npm run logo && cross-env NODE_ENV=development node server/",
    "start:prod": "npm run build:prod && cross-env NODE_ENV=production node server/",
    "build": "cross-env NODE_ENV=development webpack --config webpack/index.ts",
    "build:prod": "npm run ngc && cross-env NODE_ENV=production webpack --config webpack/index.ts",
    "generate:dll": "npm run logo && npm run clean && cross-env NODE_ENV=production webpack --config webpack/webpack.dll.config.ts",
    "clean": "rimraf dist/",
    "tslint": "tslint --exclude=src/**/*.spec.ts",
    "lint": "npm run tslint \"src/**/*.ts\"",
    "lint:css": "npm run logo && stylelint \"src/**/*.css\" --config .stylelintrc",
    "lint:all": "npm run lint && npm run lint:css",
    "test": "npm run logo && rimraf .tmp && mocha-webpack --opts src/base/conf/mocha.opts",
    "test:watch": "rimraf .tmp && mocha-webpack --watch",
    "test:coverage": "nyc npm test && cross-env NODE_ENV=development nyc report --reporter=lcov",
    "postYeomanGenerator": "npm run regenerate && npm run start",
    "yo": "npm link src/conf/.react-base/templates/generator-react-base",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/.bin/coveralls",
    "postinstall": "npm run generate:dll"
  },
  "dependencies": {
    "@angular/common": "4.0.1",
    "@angular/compiler": "4.0.1",
    "@angular/compiler-cli": "4.0.1",
    "@angular/core": "4.0.1",
    "@angular/http": "4.0.1",
    "@angular/platform-browser": "4.0.1",
    "@angular/platform-browser-dynamic": "4.0.1",
    "@angular/platform-server": "4.0.1",
    "@angular/router": "4.0.1",
    "@angular/tsc-wrapped": "4.0.1",
    "@angularclass/hmr": "1.2.2",
    "@angularclass/hmr-loader": "3.0.2",
    "@angularclass/idle-preload": "1.0.4",
    "@angularclass/resolve-angular-routes": "1.0.9",
    "@ngrx/core": "1.2.0",
    "@ngrx/effects": "2.0.2",
    "@ngrx/router-store": "1.2.5",
    "@ngrx/store": "2.2.1",
    "@ngrx/store-devtools": "3.2.4",
    "@ngrx/store-log-monitor": "3.0.2",
    "angular-router-loader": "0.6.0",
    "angular2-express-engine": "2.1.0-rc.1",
    "angular2-template-loader": "0.6.2",
    "angular2-universal": "2.1.0-rc.1",
    "angular2-universal-polyfills": "2.1.0-rc.1",
    "body-parser": "1.17.1",
    "compression": "1.6.2",
    "core-js": "2.4.1",
    "express": "4.15.2",
    "zone.js": "0.8.5"
  },
