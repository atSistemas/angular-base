{
  "compileOnSave": false,
  "buildOnSave": false,
  "compilerOptions": {
    "module": "commonjs",
    "target": "es6",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "noImplicitAny": false,
    "removeComments": false,
    "sourceMap": false,
    "inlineSourceMap": true,
    "moduleResolution": "node",
    "noEmitHelpers": true
  },
  "exclude": [
    "node_modules",
    "dist",
    "server",
    "webpack",
    "typings",
    ".git"
  ]
}
