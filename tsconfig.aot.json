{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "es2015",
      "dom"
    ],
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "noEmitHelpers": true,
    "pretty": false,
    "sourceMap": true,
    "noEmit": true,
    "strictNullChecks": false,
    "skipDefaultLibCheck": true,
    "baseUrl": "./src",
    "paths": {
      "base/*": [
        "./base/*"
      ],
      "base": [
        "./base"
      ],
      "app/*": [
        "./app/*"
      ],
      "app": [
        "./app"
      ]
    },
    "typeRoots": [
      "node_modules/@types"
    ]
  },
  "exclude": [
    "dist",
    ".base",
    "webpack",
    "node_modules",
    "src/compiled",
    "src/**/*.e2e.ts",
    "src/**/*.spec.ts",
    "server/",
    "src/base/wp-plugins",
    "src/app/index.aot.ts"
  ],
  "awesomeTypescriptLoaderOptions": {
    "useWebpackText": true,
    "forkChecker": true,
    "useCache": true,
    "silent": true
  },
  "compileOnSave": false,
  "buildOnSave": false,
  "atom": {
    "rewriteTsconfig": false
  },
  "angularCompilerOptions": {
    "genDir": "src/compiled",
    "skipMetadataEmit": true
  }
}